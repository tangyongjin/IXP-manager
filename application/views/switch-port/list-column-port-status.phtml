<td>
    {$state = $row[$cconf.colname]}
    {if $portStates.$state == 'up' }
        <span class="label label-success">Up</span>
    {else if $portStates.$state == 'down' }
        <span class="label label-important">Down</span>
    {else}
        <span class="label label-warning">{$portStates.$state}</span>
    {/if}
</td>
